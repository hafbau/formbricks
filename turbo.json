{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@fastform/database#build": {
      "cache": false,
      "dependsOn": [
        "post-install"
      ]
    },
    "@fastform/web#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@fastform/database#db:setup",
        "@fastform/js#build"
      ]
    },
    "@fastform/demo#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@fastform/js#build"
      ]
    },
    "@fastform/api#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@fastform/js#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@fastform/forms#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@fastform/forms#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@fastform/forms#build"
      ]
    },
    "@fastform/js#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@fastform/js#build"
      ]
    },
    "@fastform/api#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@fastform/api#build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "env": [
        "ASSET_PREFIX_URL",
        "AZUREAD_AUTH_ENABLED",
        "AZUREAD_CLIENT_ID",
        "AZUREAD_CLIENT_SECRET",
        "AZUREAD_TENANT_ID",
        "CRON_SECRET",
        "ENCRYPTION_KEY",
        "DEBUG",
        "GITHUB_ID",
        "GITHUB_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "GOOGLE_SHEETS_CLIENT_ID",
        "GOOGLE_SHEETS_CLIENT_SECRET",
        "GOOGLE_SHEETS_REDIRECT_URL",
        "HEROKU_APP_NAME",
        "INSTANCE_ID",
        "INTERNAL_SECRET",
        "MAIL_FROM",
        "EMAIL_VERIFICATION_DISABLED",
        "FASTFORM_ENCRYPTION_KEY",
        "GOOGLE_AUTH_ENABLED",
        "GITHUB_AUTH_ENABLED",
        "IS_FASTFORM_CLOUD",
        "PASSWORD_RESET_DISABLED",
        "PRIVACY_URL",
        "NEXT_PUBLIC_SENTRY_DSN",
        "INVITE_DISABLED",
        "SIGNUP_DISABLED",
        "NEXT_PUBLIC_STRIPE_PRICING_TABLE_ID",
        "TERMS_URL",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL",
        "SMTP_HOST",
        "SMTP_PASSWORD",
        "SMTP_PORT",
        "SMTP_SECURE_ENABLED",
        "SMTP_USER",
        "NEXT_PUBLIC_DOCSEARCH_APP_ID",
        "NEXT_PUBLIC_DOCSEARCH_API_KEY",
        "NEXT_PUBLIC_DOCSEARCH_INDEX_NAME",
        "NEXT_PUBLIC_FASTFORM_API_HOST",
        "NEXT_PUBLIC_FASTFORM_ENVIRONMENT_ID",
        "NEXT_PUBLIC_FASTFORM_FORM_ID",
        "NEXT_PUBLIC_FASTFORM_FEEDBACK_FORM_ID",
        "NEXT_PUBLIC_FASTFORM_FEEDBACK_CUSTOM_FORM_ID",
        "NEXT_PUBLIC_FASTFORM_ONBOARDING_form_ID",
        "NEXT_PUBLIC_FASTFORM_PMF_FORM_ID",
        "NEXT_PUBLIC_FASTFORM_URL",
        "IMPRINT_URL",
        "NEXT_PUBLIC_SENTRY_DSN",
        "SHORT_URL_BASE",
        "NODE_ENV",
        "NEXT_PUBLIC_POSTHOG_API_HOST",
        "NEXT_PUBLIC_POSTHOG_API_KEY",
        "NEXT_PUBLIC_FASTFORM_COM_API_HOST",
        "NEXT_PUBLIC_FASTFORM_COM_ENVIRONMENT_ID",
        "NEXT_PUBLIC_FASTFORM_COM_DOCS_FEEDBACK_form_ID",
        "WEBAPP_URL",
        "SENTRY_DSN",
        "STRAPI_API_KEY",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "TELEMETRY_DISABLED",
        "VERCEL_URL",
        "WEBAPP_URL",
        "AIRTABLE_CLIENT_ID",
        "AWS_ACCESS_KEY",
        "AWS_SECRET_KEY",
        "S3_ACCESS_KEY",
        "S3_SECRET_KEY",
        "S3_REGION",
        "S3_BUCKET_NAME",
        "ENTERPRISE_LICENSE_KEY"
      ]
    },
    "post-install": {
      "cache": false,
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "./schema.prisma"
      ]
    },
    "db:setup": {
      "cache": false,
      "outputs": []
    },
    "db:down": {
      "cache": false,
      "outputs": []
    },
    "go": {
      "persistent": true,
      "cache": false
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:migrate:vercel": {
      "env": [
        "MIGRATE_DATABASE_URL",
        "DATABASE_URL"
      ],
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "db:start": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}